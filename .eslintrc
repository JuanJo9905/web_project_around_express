{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": "airbnb-base",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "linebreak-style": ["error", "unix"]
  },
  "languageOptions": {
    "globals": {
      "browser": true
    }
  }
}
